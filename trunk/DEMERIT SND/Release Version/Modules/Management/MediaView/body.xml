<?xml version="1.0" encoding="UTF-8"?>
<panel layout="borderlayout">
	<panel layout="GridLayout(0,2,10,10)" id="vbxMediaView" constraints="BorderLayout.CENTER" visible="false">
		<vbox>
			<panel layout="FlowLayout(FlowLayout.LEFT)">
				<label font="ARIAL-BOLD-20" id="lblTitle"></label>
			</panel>
			<panel layout="FlowLayout(FlowLayout.LEFT)" id="pnlType">
				<label font="ARIAL-BOLD-14" preferredsize="105, 30">Type</label>
				<label></label>
				<combobox preferredsize="200, 30" visible="false" id="cbbType"></combobox>
				<linklabel>change</linklabel>
			</panel>
			<panel layout="FlowLayout(FlowLayout.LEFT)" id="pnlFormat">
				<label font="ARIAL-BOLD-14" preferredsize="105, 30">Format</label>
				<label></label>
				<combobox preferredsize="200, 30" visible="false" id="cbbFormat"></combobox>
				<linklabel>change</linklabel>
			</panel>
			<panel layout="FlowLayout(FlowLayout.LEFT)" id="pnlGenre">
				<label font="ARIAL-BOLD-14" preferredsize="105, 30">Genre</label>
				<label></label>
				<combobox preferredsize="200, 30" visible="false" id="cbbGenre"></combobox>
				<linklabel>change</linklabel>
			</panel>
			<panel layout="FlowLayout(FlowLayout.LEFT)" id="pnlLength">
				<label font="ARIAL-BOLD-14" preferredsize="105, 30">Length</label>
				<label></label>
				<textfield preferredsize="200, 30" visible="false"></textfield>
				<linklabel>change</linklabel>
			</panel>
			<panel layout="FlowLayout(FlowLayout.LEFT)" id="pnlCallNumber">
				<label font="ARIAL-BOLD-14" preferredsize="105, 30">Call #</label>
				<label></label>
				<textfield preferredsize="200, 30" visible="false"></textfield>
				<linklabel>change</linklabel>
			</panel>
			<panel layout="FlowLayout(FlowLayout.LEFT)" id="pnlBarcode">
				<label font="ARIAL-BOLD-14" preferredsize="105, 30">Barcode</label>
				<label></label>
				<textfield preferredsize="200, 30" visible="false"></textfield>
				<linklabel>change</linklabel>
			</panel>
			<panel layout="FlowLayout(FlowLayout.LEFT)" id="pnlCopyright">
				<label font="ARIAL-BOLD-14" preferredsize="105, 30">Copyright</label>
				<label></label>
				<textfield preferredsize="200, 30" visible="false"></textfield>
				<linklabel>change</linklabel>
			</panel>
			<panel layout="FlowLayout(FlowLayout.LEFT)" id="pnlEdition">
				<label font="ARIAL-BOLD-14" preferredsize="105, 30">Edition</label>
				<label></label>
				<textfield preferredsize="200, 30" visible="false"></textfield>
				<linklabel>change</linklabel>
			</panel>
			<panel layout="FlowLayout(FlowLayout.LEFT)" id="pnlISBN">
				<label font="ARIAL-BOLD-14" preferredsize="105, 30">ISBN</label>
				<label></label>
				<textfield preferredsize="200, 30" visible="false"></textfield>
				<linklabel>change</linklabel>
			</panel>
		</vbox>
		<vbox>
			<panel layout="FlowLayout(FlowLayout.LEFT)" id="pnlDescription">
				<label font="ARIAL-BOLD-14" preferredsize="105, 30">Description</label>
				<label></label>
				<textarea preferredsize="250,80" visible="false"></textarea>
				<linklabel>change</linklabel>
			</panel>
			<panel layout="FlowLayout(FlowLayout.LEFT)" id="pnlProducers">
				<label font="ARIAL-BOLD-14" preferredsize="105, 30">Producers</label>
				<label></label>
				<autocompletescroll id="autProducers" preferredsize="300, 70" visible="false"></autocompletescroll>
				<linklabel>change</linklabel>
			</panel>
			<panel layout="FlowLayout(FlowLayout.LEFT)" id="pnlActors">
				<label font="ARIAL-BOLD-14" preferredsize="105, 30">Actors</label>
				<label></label>
				<autocompletescroll id="autActors" preferredsize="300, 70" visible="false"></autocompletescroll>
				<linklabel>change</linklabel>
			</panel>
			<panel layout="FlowLayout(FlowLayout.LEFT)" id="pnlAuthors">
				<label font="ARIAL-BOLD-14" preferredsize="105, 30">Authors</label>
				<label></label>
				<autocompletescroll id="autAuthors" preferredsize="300, 70" visible="false"></autocompletescroll>
				<linklabel>change</linklabel>
			</panel>
			<panel layout="FlowLayout(FlowLayout.LEFT)" id="pnlCategories">
				<label font="ARIAL-BOLD-14" preferredsize="105, 30">Categories</label>
				<label></label>
				<autocompletescroll id="autCategories" preferredsize="300, 70" visible="false"></autocompletescroll>
				<linklabel>change</linklabel>
			</panel>
		</vbox>
	</panel>
</panel>